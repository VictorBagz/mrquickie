{
  "name": "mr-quickie-website",
  "version": "1.0.0",
  "description": "Mr Quickie business website with services, products, and interactive features",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000 --open=/index.html",
    "build": "npm run minify-css && npm run minify-js",
    "minify-css": "cleancss -o dist/styles.min.css styles.css",
    "minify-js": "uglifyjs scripts.js -o dist/scripts.min.js -c -m",
    "optimize-images": "imagemin images/* --out-dir=dist/images",
    "lint-css": "stylelint **/*.css",
    "lint-js": "eslint **/*.js",
    "fix-js": "eslint **/*.js --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run dev",
    "clean": "rimraf dist",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "nodemon --watch styles.css --exec \"npm run minify-css\"",
    "watch:js": "nodemon --watch scripts.js --exec \"npm run minify-js\"",
    "serve": "http-server -p 8080 -c-1",
    "format": "prettier --write **/*.{html,css,js,json,md}"
  },
  "keywords": [
    "business-website",
    "responsive-design",
    "javascript",
    "css",
    "html",
    "slider",
    "woocommerce",
    "wordpress-theme"
  ],
  "author": "Mr Quickie",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "http-server": "^14.1.1",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "live-server": "^1.2.2",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "stylelint": "^16.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
    "uglify-js": "^3.17.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "slick-carousel": "^1.8.1",
    "aos": "^2.3.4",
    "swiper": "^11.0.5",
    "gsap": "^3.12.4",
    "locomotive-scroll": "^4.1.4",
    "vanilla-lazyload": "^17.8.8",
    "intersection-observer": "^0.12.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "IE 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
